box-shadow: inset 0 0 40px rgba(0, 229, 255,1);}\r\n    100% { border: 2px solid rgba(118, 255, 3,0.8);}\r\n\r\n}\r\n\r\n//prepend icon \r\n\r\n .icon{\r\n     background-color: rgba(29, 233, 182);\r\n     padding: 5px;\r\n     box-shadow: 5px 5px 12px rgba(0,0,0,0.1), 5px 5px 12px rgba(0,0,0,0.1);\r\n     margin-left: 10px;\r\n     border: 2px solid #fff;\r\n }\r\n\r\n .icon img{\r\n     height: 20px;\r\n     width: 20px;\r\n }\r\n\r\n .logo img{\r\n    height: 40px;\r\n    width: 40px;\r\n }\r\n\r\n //end of prepend icon\r\n input::placeholder{\r\n     margin-left: 8px;\r\n     color:#fff;\r\n     font-size: 12px;\r\n }\r\n\r\n .forgotpass a:hover{\r\n    text-decoration: none;\r\n    color:#1DE9B6;\r\n }\r\n\r\n .forgotpass a{\r\n    text-decoration: none;\r\n    margin-top: 15px;\r\n    font-size: 12px;\r\n    font-family: 'Ubuntu Mono', monospace;\r\n    color:#E0E0E0;\r\n    margin-left: 100px;\r\n }\r\n\r\n.nav h3{\r\n    left: 10px;\r\n    padding: 10px;\r\n}\r\n\r\n.title h3{\r\n    padding: 10px;\r\n    left: 10px;\r\n    position: absolute;\r\n\r\n}\r\n\r\n.nav li{\r\n    text-align: center;\r\n    padding:10px;\r\n    border: 1px solid #e0e0e0;\r\n}"},"lineCount":null}},"error":null,"hash":"1f4248232db39232533fcedfb6e50fb4","cacheData":{"env":{}}}